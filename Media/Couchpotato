docker run -d \
  --name=couchpotato \
  -e PUID=1000 \
  -e PGID=1000 \
  -e TZ=Europe/Madrid \
  -p 5050:5050 \
  -v ~/Docker/couchpotato/config:/config \
  -v ~/Downloads/couchpotato/downloads:/downloads \
  -v ~/Downloads/couchpotato/movies:/movies \
  --restart unless-stopped \
  #ghcr.io/linuxserver/couchpotato
